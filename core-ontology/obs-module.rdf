<?xml version="1.0"?>
<rdf:RDF xmlns="https://www.loa.istc.cnr.it/obs#"
     xml:base="https://www.loa.istc.cnr.it/obs"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:swrl="http://www.w3.org/2003/11/swrl#"
     xmlns:swrlb="http://www.w3.org/2003/11/swrlb#">
    <owl:Ontology rdf:about="https://www.loa.istc.cnr.it/obs">
        <rdfs:comment>Core module</rdfs:comment>
        <rdfs:label>Observational ontology</rdfs:label>
        <owl:versionInfo>Created by: Emilio M. Sanfilippo, Claudio Masolo, and Alessandro Mosca in the context of the research project &quot;MITE - Make it explicit: Documenting interpretations of literary fictions with conceptual formal models&quot; (2023-2025) funded by the European Union - Next Generation EU.</owl:versionInfo>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- https://www.loa.istc.cnr.it/obs#argumentOf -->

    <owl:ObjectProperty rdf:about="https://www.loa.istc.cnr.it/obs#argumentOf">
        <owl:inverseOf rdf:resource="https://www.loa.istc.cnr.it/obs#hasArgument"/>
        <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:range rdf:resource="https://www.loa.istc.cnr.it/obs#Observation"/>
        <rdfs:label>argument of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://www.loa.istc.cnr.it/obs#assertChainRootIn -->

    <owl:ObjectProperty rdf:about="https://www.loa.istc.cnr.it/obs#assertChainRootIn">
        <rdfs:subPropertyOf rdf:resource="https://www.loa.istc.cnr.it/obs#sourceChainRootIn"/>
        <rdfs:domain rdf:resource="https://www.loa.istc.cnr.it/obs#AssertObs"/>
        <rdfs:comment>assertChainRootIn(a,o) - a is an assertion chain of observations rooted in o</rdfs:comment>
        <rdfs:label>assertion chain rooted in</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://www.loa.istc.cnr.it/obs#hasArgument -->

    <owl:ObjectProperty rdf:about="https://www.loa.istc.cnr.it/obs#hasArgument">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="https://www.loa.istc.cnr.it/obs#Observation"/>
        <rdfs:range rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:label>has argument</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://www.loa.istc.cnr.it/obs#hasDefeatedObs -->

    <owl:ObjectProperty rdf:about="https://www.loa.istc.cnr.it/obs#hasDefeatedObs">
        <rdfs:subPropertyOf rdf:resource="https://www.loa.istc.cnr.it/obs#hasObs"/>
        <rdfs:domain rdf:resource="https://www.loa.istc.cnr.it/obs#DefeatObs"/>
        <rdfs:range rdf:resource="https://www.loa.istc.cnr.it/obs#AtomicObs"/>
        <owl:propertyDisjointWith rdf:resource="https://www.loa.istc.cnr.it/obs#hasDefeatingObs"/>
        <rdfs:label>has defeated observation</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://www.loa.istc.cnr.it/obs#hasDefeatingObs -->

    <owl:ObjectProperty rdf:about="https://www.loa.istc.cnr.it/obs#hasDefeatingObs">
        <rdfs:subPropertyOf rdf:resource="https://www.loa.istc.cnr.it/obs#hasObs"/>
        <rdfs:domain rdf:resource="https://www.loa.istc.cnr.it/obs#DefeatObs"/>
        <rdfs:label>has defeating observation</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://www.loa.istc.cnr.it/obs#hasObs -->

    <owl:ObjectProperty rdf:about="https://www.loa.istc.cnr.it/obs#hasObs">
        <rdfs:subPropertyOf rdf:resource="https://www.loa.istc.cnr.it/obs#hasArgument"/>
        <owl:inverseOf rdf:resource="https://www.loa.istc.cnr.it/obs#observationOf"/>
        <rdfs:range rdf:resource="https://www.loa.istc.cnr.it/obs#Observation"/>
        <rdfs:label>has observation</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://www.loa.istc.cnr.it/obs#hasPart -->

    <owl:ObjectProperty rdf:about="https://www.loa.istc.cnr.it/obs#hasPart">
        <owl:inverseOf rdf:resource="https://www.loa.istc.cnr.it/obs#partOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:range rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:label>has part</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://www.loa.istc.cnr.it/obs#hasProperPart -->

    <owl:ObjectProperty rdf:about="https://www.loa.istc.cnr.it/obs#hasProperPart">
        <rdfs:subPropertyOf rdf:resource="https://www.loa.istc.cnr.it/obs#hasPart"/>
        <owl:inverseOf rdf:resource="https://www.loa.istc.cnr.it/obs#properPartOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:label>has proper part</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://www.loa.istc.cnr.it/obs#hasSource -->

    <owl:ObjectProperty rdf:about="https://www.loa.istc.cnr.it/obs#hasSource">
        <rdfs:subPropertyOf rdf:resource="https://www.loa.istc.cnr.it/obs#hasArgument"/>
        <rdfs:domain rdf:resource="https://www.loa.istc.cnr.it/obs#SourceObs"/>
        <rdfs:range rdf:resource="https://www.loa.istc.cnr.it/obs#Text"/>
        <rdfs:comment>source of rejection or assertion</rdfs:comment>
        <rdfs:label>has source</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://www.loa.istc.cnr.it/obs#hasSupportedObs -->

    <owl:ObjectProperty rdf:about="https://www.loa.istc.cnr.it/obs#hasSupportedObs">
        <rdfs:subPropertyOf rdf:resource="https://www.loa.istc.cnr.it/obs#hasObs"/>
        <rdfs:domain rdf:resource="https://www.loa.istc.cnr.it/obs#SupportObs"/>
        <rdfs:range rdf:resource="https://www.loa.istc.cnr.it/obs#AtomicObs"/>
        <owl:propertyDisjointWith rdf:resource="https://www.loa.istc.cnr.it/obs#hasSupportingObs"/>
        <rdfs:label>has supported observation</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://www.loa.istc.cnr.it/obs#hasSupportingObs -->

    <owl:ObjectProperty rdf:about="https://www.loa.istc.cnr.it/obs#hasSupportingObs">
        <rdfs:subPropertyOf rdf:resource="https://www.loa.istc.cnr.it/obs#hasObs"/>
        <rdfs:domain rdf:resource="https://www.loa.istc.cnr.it/obs#SupportObs"/>
        <rdfs:label>has supporting observation</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://www.loa.istc.cnr.it/obs#kbAssertChainRootIn -->

    <owl:ObjectProperty rdf:about="https://www.loa.istc.cnr.it/obs#kbAssertChainRootIn">
        <rdfs:subPropertyOf rdf:resource="https://www.loa.istc.cnr.it/obs#assertChainRootIn"/>
        <rdfs:subPropertyOf rdf:resource="https://www.loa.istc.cnr.it/obs#kbSourceChainRootIn"/>
        <rdfs:domain rdf:resource="https://www.loa.istc.cnr.it/obs#KBAssertObs"/>
        <rdfs:label>kb assertion chain rooted in</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://www.loa.istc.cnr.it/obs#kbSourceChainRootIn -->

    <owl:ObjectProperty rdf:about="https://www.loa.istc.cnr.it/obs#kbSourceChainRootIn">
        <rdfs:subPropertyOf rdf:resource="https://www.loa.istc.cnr.it/obs#sourceChainRootIn"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://www.loa.istc.cnr.it/obs#KBAssertObs"/>
                    <rdf:Description rdf:about="https://www.loa.istc.cnr.it/obs#KBRejectObs"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:label>kb source chain rooted in</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://www.loa.istc.cnr.it/obs#observationOf -->

    <owl:ObjectProperty rdf:about="https://www.loa.istc.cnr.it/obs#observationOf">
        <rdfs:subPropertyOf rdf:resource="https://www.loa.istc.cnr.it/obs#argumentOf"/>
        <rdfs:domain rdf:resource="https://www.loa.istc.cnr.it/obs#Observation"/>
        <rdfs:label>observation of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://www.loa.istc.cnr.it/obs#partOf -->

    <owl:ObjectProperty rdf:about="https://www.loa.istc.cnr.it/obs#partOf">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:range rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:label>part of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://www.loa.istc.cnr.it/obs#properPartOf -->

    <owl:ObjectProperty rdf:about="https://www.loa.istc.cnr.it/obs#properPartOf">
        <rdfs:subPropertyOf rdf:resource="https://www.loa.istc.cnr.it/obs#partOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:label>proper part of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://www.loa.istc.cnr.it/obs#sourceChainRootIn -->

    <owl:ObjectProperty rdf:about="https://www.loa.istc.cnr.it/obs#sourceChainRootIn">
        <rdfs:domain rdf:resource="https://www.loa.istc.cnr.it/obs#SourceObs"/>
        <rdfs:range rdf:resource="https://www.loa.istc.cnr.it/obs#Observation"/>
        <rdfs:comment>sourceChainRootIn(a,o) - a is a source chain of observations rooted in o</rdfs:comment>
        <rdfs:label>source chain rooted in</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- https://www.loa.istc.cnr.it/obs#ArgumentationObs -->

    <owl:Class rdf:about="https://www.loa.istc.cnr.it/obs#ArgumentationObs">
        <owl:equivalentClass>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://www.loa.istc.cnr.it/obs#DefeatObs"/>
                    <rdf:Description rdf:about="https://www.loa.istc.cnr.it/obs#SupportObs"/>
                </owl:unionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="https://www.loa.istc.cnr.it/obs#AtomicObs"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://www.loa.istc.cnr.it/obs#hasObs"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>An argumentation observation supports or defeats another observation.</rdfs:comment>
        <rdfs:label>Argumentation Observation</rdfs:label>
    </owl:Class>
    


    <!-- https://www.loa.istc.cnr.it/obs#AssertObs -->

    <owl:Class rdf:about="https://www.loa.istc.cnr.it/obs#AssertObs">
        <rdfs:subClassOf rdf:resource="https://www.loa.istc.cnr.it/obs#SourceObs"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://www.loa.istc.cnr.it/obs#hasArgument"/>
                <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">2</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://www.loa.istc.cnr.it/obs#hasObs"/>
                <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://www.loa.istc.cnr.it/obs#hasSource"/>
                <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="https://www.loa.istc.cnr.it/obs#DefeatObs"/>
        <owl:disjointWith rdf:resource="https://www.loa.istc.cnr.it/obs#RejectObs"/>
        <owl:disjointWith rdf:resource="https://www.loa.istc.cnr.it/obs#SupportObs"/>
        <rdfs:label>Assert Observation</rdfs:label>
    </owl:Class>
    


    <!-- https://www.loa.istc.cnr.it/obs#AtomicObs -->

    <owl:Class rdf:about="https://www.loa.istc.cnr.it/obs#AtomicObs">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://www.loa.istc.cnr.it/obs#Observation"/>
                    <owl:Class>
                        <owl:complementOf>
                            <owl:Restriction>
                                <owl:onProperty>
                                    <rdf:Description>
                                        <owl:inverseOf rdf:resource="https://www.loa.istc.cnr.it/obs#properPartOf"/>
                                    </rdf:Description>
                                </owl:onProperty>
                                <owl:someValuesFrom rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
                            </owl:Restriction>
                        </owl:complementOf>
                    </owl:Class>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty>
                    <rdf:Description>
                        <owl:inverseOf rdf:resource="https://www.loa.istc.cnr.it/obs#kbSourceChainRootIn"/>
                    </rdf:Description>
                </owl:onProperty>
                <owl:someValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="https://www.loa.istc.cnr.it/obs#KBAssertObs"/>
                            <rdf:Description rdf:about="https://www.loa.istc.cnr.it/obs#KBRejectObs"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>An atomic observation that does not have other observation as part</rdfs:comment>
        <rdfs:label>Atomic observation</rdfs:label>
    </owl:Class>
    


    <!-- https://www.loa.istc.cnr.it/obs#BasicObs -->

    <owl:Class rdf:about="https://www.loa.istc.cnr.it/obs#BasicObs">
        <rdfs:subClassOf rdf:resource="https://www.loa.istc.cnr.it/obs#AtomicObs"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://www.loa.istc.cnr.it/obs#hasArgument"/>
                <owl:someValuesFrom>
                    <owl:Class>
                        <owl:complementOf rdf:resource="https://www.loa.istc.cnr.it/obs#Observation"/>
                    </owl:Class>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://www.loa.istc.cnr.it/obs#hasArgument"/>
                <owl:allValuesFrom>
                    <owl:Class>
                        <owl:complementOf rdf:resource="https://www.loa.istc.cnr.it/obs#Observation"/>
                    </owl:Class>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>A basic observation is an observation that concerns entities (has arguments) that are not observations</rdfs:comment>
        <rdfs:label>Basic Observation</rdfs:label>
    </owl:Class>
    


    <!-- https://www.loa.istc.cnr.it/obs#ConflictualObs -->

    <owl:Class rdf:about="https://www.loa.istc.cnr.it/obs#ConflictualObs">
        <rdfs:subClassOf rdf:resource="https://www.loa.istc.cnr.it/obs#Observation"/>
        <rdfs:comment>A conflictual observation is a compound observation whose parts are conflicting each other. Instances of this class are automatically inferred through a dedicated SWRL rule.</rdfs:comment>
        <rdfs:label>Conflictual Observation</rdfs:label>
    </owl:Class>
    


    <!-- https://www.loa.istc.cnr.it/obs#DefeatObs -->

    <owl:Class rdf:about="https://www.loa.istc.cnr.it/obs#DefeatObs">
        <rdfs:subClassOf rdf:resource="https://www.loa.istc.cnr.it/obs#ArgumentationObs"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://www.loa.istc.cnr.it/obs#hasArgument"/>
                <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">2</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://www.loa.istc.cnr.it/obs#hasDefeatedObs"/>
                <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://www.loa.istc.cnr.it/obs#hasDefeatingObs"/>
                <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="https://www.loa.istc.cnr.it/obs#RejectObs"/>
        <owl:disjointWith rdf:resource="https://www.loa.istc.cnr.it/obs#SupportObs"/>
        <rdfs:label>Defeat Observation</rdfs:label>
    </owl:Class>
    


    <!-- https://www.loa.istc.cnr.it/obs#DisputableObs -->

    <owl:Class rdf:about="https://www.loa.istc.cnr.it/obs#DisputableObs">
        <rdfs:subClassOf rdf:resource="https://www.loa.istc.cnr.it/obs#Observation"/>
        <rdfs:comment>Instances of this class are automatically inferred reasoning over a dedicated SWRL rule</rdfs:comment>
        <rdfs:comment>A disputable observation is an observation that is asserted and rejected.</rdfs:comment>
        <rdfs:label>Disputable Observation</rdfs:label>
    </owl:Class>
    


    <!-- https://www.loa.istc.cnr.it/obs#IncoherentSource -->

    <owl:Class rdf:about="https://www.loa.istc.cnr.it/obs#IncoherentSource">
        <rdfs:subClassOf rdf:resource="https://www.loa.istc.cnr.it/obs#Text"/>
        <rdfs:comment>An incoherent source is a source that asserts and rejects the same observation</rdfs:comment>
        <rdfs:label>Incoherent Source</rdfs:label>
    </owl:Class>
    


    <!-- https://www.loa.istc.cnr.it/obs#KBAssertObs -->

    <owl:Class rdf:about="https://www.loa.istc.cnr.it/obs#KBAssertObs">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://www.loa.istc.cnr.it/obs#AssertObs"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://www.loa.istc.cnr.it/obs#hasSource"/>
                        <owl:hasValue rdf:resource="https://www.loa.istc.cnr.it/obs#KB"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf>
            <owl:Class>
                <owl:complementOf>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://www.loa.istc.cnr.it/obs#observationOf"/>
                        <owl:someValuesFrom>
                            <owl:Class>
                                <owl:unionOf rdf:parseType="Collection">
                                    <rdf:Description rdf:about="https://www.loa.istc.cnr.it/obs#AssertObs"/>
                                    <rdf:Description rdf:about="https://www.loa.istc.cnr.it/obs#RejectObs"/>
                                </owl:unionOf>
                            </owl:Class>
                        </owl:someValuesFrom>
                    </owl:Restriction>
                </owl:complementOf>
            </owl:Class>
        </rdfs:subClassOf>
        <rdfs:comment>Assert Observation with source the knowledge base. This class is introduced for technical reasons.</rdfs:comment>
        <rdfs:label>KB Assert Observation</rdfs:label>
    </owl:Class>
    


    <!-- https://www.loa.istc.cnr.it/obs#KBRejectObs -->

    <owl:Class rdf:about="https://www.loa.istc.cnr.it/obs#KBRejectObs">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://www.loa.istc.cnr.it/obs#RejectObs"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://www.loa.istc.cnr.it/obs#hasSource"/>
                        <owl:hasValue rdf:resource="https://www.loa.istc.cnr.it/obs#KB"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf>
            <owl:Class>
                <owl:complementOf>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://www.loa.istc.cnr.it/obs#observationOf"/>
                        <owl:someValuesFrom>
                            <owl:Class>
                                <owl:unionOf rdf:parseType="Collection">
                                    <rdf:Description rdf:about="https://www.loa.istc.cnr.it/obs#AssertObs"/>
                                    <rdf:Description rdf:about="https://www.loa.istc.cnr.it/obs#RejectObs"/>
                                </owl:unionOf>
                            </owl:Class>
                        </owl:someValuesFrom>
                    </owl:Restriction>
                </owl:complementOf>
            </owl:Class>
        </rdfs:subClassOf>
        <rdfs:comment>Reject Observation with source the knowledge base. This class is introduced for technical reasons.</rdfs:comment>
        <rdfs:label>KB Reject Observation</rdfs:label>
    </owl:Class>
    


    <!-- https://www.loa.istc.cnr.it/obs#Observation -->

    <owl:Class rdf:about="https://www.loa.istc.cnr.it/obs#Observation">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://www.loa.istc.cnr.it/obs#hasArgument"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://www.loa.istc.cnr.it/obs#partOf"/>
                <owl:allValuesFrom rdf:resource="https://www.loa.istc.cnr.it/obs#Observation"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty>
